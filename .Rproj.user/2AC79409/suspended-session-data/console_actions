        "  i.p <- gerapop(203, pop, pop_total, cas, casos_total, geo, a)",
        "+ ",
        "  proc.time()",
        "+ ",
        "  r <- bpso(llrpen, i.p, pop, pop_total, cas, casos_total, geo, adj, estrutura, a)",
        "+ ",
        "  llrh0[i] <- r[[3]]",
        "+ ",
