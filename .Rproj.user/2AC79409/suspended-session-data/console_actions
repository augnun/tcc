        "> ",
        "sens2 <- sens2 / pow2",
        "> ",
        "ppv2 <- ppv2 / pow2",
        "> ",
        "pow2 <- pow2 / nsim",
        "> ",
        "",
        "> ",
        "",
        "> ",
        "######## ",
        "> ",
        "#CASO 3",
        "> ",
        "real <- read.table(\"c3.txt\") # cluster verdadeiro",
        "> ",
        "cases <- read.table(\"cases3.txt\") # casos gerados sob Ha",
        "> ",
        "",
        "> ",
        "adj <- read.table(\"hex.adj\") # Matriz de Adjascência",
        "> ",
