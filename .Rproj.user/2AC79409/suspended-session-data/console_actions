        "> ",
        "",
        "> ",
        "# simulacao sob H0 para computar valor critico",
        "> ",
        "# (quantil 95% da dist. da estatistica de teste sob H0)",
        "> ",
        "",
        "> ",
        "llrh0 <- rep(0, nsim)",
        "> ",
        "prob <-",
        "+ ",
        "  pop[, 1] / pop_total # sob H0, a probabilidade em cada regiao e proporcional a populacao",
        "> ",
        "time <- proc.time()",
        "> ",
        "for (i in 1:nsim) {",
        "+ ",
        "  print(i)",
